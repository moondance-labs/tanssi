{
  "$schema": "https://raw.githubusercontent.com/Moonsong-Labs/moonwall/main/packages/cli/config_schema.json",
  "label": "MasterConfig ðŸŒ”",
  "defaultTestTimeout": 30000,
  "environments": [
    {
      "name": "dev_tanssi",
      "testFileDir": ["suites/dev"],
      "multiThreads": true,
      "html": false,
      "foundation": {
        "type": "dev",
        "launchSpec": [
          {
            "name": "tanssi",
            "binPath": "../target/release/tanssi-node",
            "options": [
                "--dev",
                "--sealing=manual"
            ]
          }
        ]
      }
    },
    {
      "name": "dev_frontier_template",
      "testFileDir": ["suites/dev-frontier-template"],
      "multiThreads": true,
      "html": false,
      "foundation": {
        "type": "dev",
        "launchSpec": [
          {
            "name": "frontier-template",
            "binPath": "../target/release/tanssi-node",
            "options": [
                "run-container-command",
                "ethereum",
                "--dev",
                "--sealing=manual"
            ]
          }
        ]
      }
    },
    {
      "name": "zombie_tanssi_ci",
      "testFileDir": ["suites/para"],
      "foundation": {
        "type": "zombie",
        "zombieSpec": {
          "configPath": "./configs/zombieTanssiCI.json"
        }
      },
      "connections": [
        {
          "name": "Relay",
          "type": "polkadotJs",
          "endpoints": ["ws://127.0.0.1:9947"]
        },
        {
          "name": "Tanssi",
          "type": "polkadotJs",
          "endpoints": ["ws://127.0.0.1:9948"]
        },
        {
          "name": "Container2000",
          "type": "polkadotJs",
          "endpoints": ["ws://127.0.0.1:9949"]
        },
        {
          "name": "Container2001",
          "type": "polkadotJs",
          "endpoints": ["ws://127.0.0.1:9950"]
        },
        {
          "name": "ethers",
          "type": "ethers",
          "endpoints": ["ws://127.0.0.1:9960"]
        },
        {
          "name": "w3",
          "type": "web3",
          "endpoints": ["ws://127.0.0.1:9960"]
        }
      ]
    },
    {
      "name": "zombie_tanssi",
      "testFileDir": ["suites/para"],
      "foundation": {
        "type": "zombie",
        "zombieSpec": {
          "configPath": "./configs/zombieTanssi.json"
        }
      },
      "connections": [
        {
          "name": "Relay",
          "type": "polkadotJs",
          "endpoints": ["ws://127.0.0.1:9947"]
        },
        {
          "name": "Tanssi",
          "type": "polkadotJs",
          "endpoints": ["ws://127.0.0.1:9948"]
        },
        {
          "name": "Container2000",
          "type": "polkadotJs",
          "endpoints": ["ws://127.0.0.1:9949"]
        },
        {
          "name": "Container2001",
          "type": "polkadotJs",
          "endpoints": ["ws://127.0.0.1:9950"]
        },
        {
          "name": "ethers",
          "type": "ethers",
          "endpoints": ["ws://127.0.0.1:9960"]
        },
        {
          "name": "w3",
          "type": "web3",
          "endpoints": ["ws://127.0.0.1:9960"]
        }
      ]
    }
  ]
